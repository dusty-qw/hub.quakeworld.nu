{
  "name": "qw-server-browser",
  "version": "0.1.0",
  "description": "Muh.",
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": "vikpe/qw-server-browser",
  "scripts": {
    "dev": "run-s clean dev:*",
    "dev:jekyll": "yarn build:jekyll --config=_config.yml,_config.dev.yml",
    "dev:assets": "gulp assets:dev",
    "watch": "run-p 'dev:jekyll --watch --incremental' 'gulp watch' start",
    "build": "run-s clean build:assets build:jekyll build:apps",
    "build:jekyll": "cd jekyll && bundle exec jekyll build",
    "build:assets": "gulp assets:prod",
    "build:apps": "snowpack build",
    "clean": "rm -rf build && cd jekyll && bundle exec jekyll clean",
    "start:sync": "browser-sync start --config bs-config.js",
    "start": "snowpack dev",
    "gulp": "gulp",
    "lint": "yarn prettier --write src/js",
    "test": "jest --config=jest.setup.js"
  },
  "devDependencies": {
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "browser-sync": "^2.26.14",
    "bs-html-injector": "^3.0.3",
    "jest": "^27.0.4",
    "prettier": "^2.3.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@snowpack/app-scripts-react": "^2.0.1",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "bulma": "^0.9.2",
    "firebase-tools": "^9.12.1",
    "formik": "^2.2.9",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "sass": "^1.34.1",
    "snowpack": "^3.5.6"
  }
}
